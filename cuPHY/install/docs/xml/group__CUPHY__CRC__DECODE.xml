      <sectiondef kind="func">
      <memberdef kind="function" id="group__CUPHY__CRC__DECODE_1ga6edcf269acda1c71dd860be27e758d0e" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
