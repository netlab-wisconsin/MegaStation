        <argsstring>(cuphyCsirsTxHndl_t *pCsirsTxHndl, cuphyCsirsStatPrms_t const *pStatPrms)</argsstring>
        <name>cuphyCreateCsirsTx</name>
        <param>
          <type><ref refid="group__CUPHY__CSIRS__TRANSMITTER_1gadc3eb0dd5da2c7182711db6cb26229ed" kindref="member">cuphyCsirsTxHndl_t</ref> *</type>
